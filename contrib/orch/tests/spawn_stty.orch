
timeout(3)

spawn("cat")
-- Set -ICANON; this will timeout if canonicalization is on, since we haven't
-- sent an EOF.
stty("lflag", 0, tty.lflag.ICANON)

write "Hello the"
match "^Hello the$"

-- Also test cc changes
spawn("cat")

stty("cc", {
	VEOF = "^F"
})
write "Hello the^F"

match "^Hello the$"
