timeout(1)

-- Demonstrate that only one of the matches will proceed.
write "ZOO\r"
one(function()
	match "OO"
	match "ZOO" {
		callback = function()
			-- Will fail and timeout
			match "Monkies"
		end
	}
end)

-- Demonstrate that it's always the first one, regardless of length or position.
write "ZOO\r"
one(function()
	match "ZOO"
	match "OO" {
		callback = function()
			-- Will fail and timeout
			match "Monkies"
		end
	}
end)
